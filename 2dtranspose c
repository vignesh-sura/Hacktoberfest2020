#include<stdio.h>

int main()
{
    int row=0,col=0;
    int i=0,j=0;
    int arr[10][10];

    printf("Enter row and col\n");
    scanf("%d %d",&row,&col);

    printf("enter matrix value\n");

    for(i=0;i<row;i++)
    {
        for(j=0;j<col;j++)
        {
            scanf("%d",&arr[i][j]);

        }
    }

    printf("the  matrix  is\n");

    for(i=0;i<row;i++)
    {
        for(j=0;j<col;j++)
        {
            printf("%d\t",arr[i][j]);

        }

        printf("\n");

    }


    printf("the  matrix transpose  is\n");

    for(j=0;j<col;j++)
    {
        for(i=0;i<row;i++)
        {
            printf("%d\t",arr[i][j]);

        }

        printf("\n");

    }









    return 0;
}
